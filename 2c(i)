#include <iostream>
using namespace std;
int main(){
    int arr[5];
    cout<<"Provide array elements :";
    for(int i=0;i<5;i++)
    {
        cin>>arr[i];
    }
    int a=arr[0],b=-1;
    for(int i=0;i<5;i++){
    if(a<arr[i]?(a=arr[i]):(a=a));}
    
    for(int i=0;i<5;i++){
        if(arr[i]>b&&arr[i]<a){ 
             b=arr[i];

        }
        
    }
    cout<<"The second largest number is:"<<b;   
   
}
