#include <iostream>
using namespace std;
int main(){
   int x,count = 0;
   cout<<"Provide a number :";
   cin>>x;
   if(x==1)
   {
    cout<<"Not a prime number.";
   }
   else if(x==2){
    cout<<"Prime number.";
   }
   else
   {
    for(int i=2;i<x;i++)
    {
    if(x%i==0){
        count++;

    }
  
   }
       if(count>0){
           cout<< "Not a prime number.";
       }
       else
           cout<<"prime number.";
   }
       return 0;
}
