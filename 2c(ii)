#include <iostream>
using namespace std;
int main(){
    int arr[5],i;
    cout<<"Provide array elements :";
    for(i=0;i<5;i++)
    {
        cin>>arr[i];
    }
    int a=arr[0];
    for(i=0;i<5;i++){
    if(a>arr[i]?(a=arr[i]):(a=a));}
    int b=676;
    for(i=0;i<5;i++)
    {
        if(arr[i]>a&&arr[i]<b){
             b=arr[i];
        }
    }
    cout<<"The second smallest number is:"<<b;
   
}
